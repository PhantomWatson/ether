<IfModule mod_security.c>
    SecFilterEngine Off
</IfModule>

<IfModule mod_rewrite.c>
    RewriteEngine On

    # Redirect HTTP to HTTPS
    RewriteCond %{HTTPS} !=on
    RewriteRule ^/?(.*) https://%{SERVER_NAME}/$1 [R=301,L]

    # Redirect from .info to .com
    RewriteCond %{HTTP_HOST} ^theether\.info$ [OR]
    RewriteCond %{HTTP_HOST} ^www\.theether\.info$
    RewriteRule ^(.*)$ "http\:\/\/theether\.com\/$1" [R=301,L]

    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteRule ^ index.php [L]
</IfModule>
