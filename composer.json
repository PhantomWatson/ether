{
	"name" : "phantomwatson/ether",
	"description" : "The Ether - Thought Repository",
	"homepage" : "http://theether.com",
	"type" : "project",
	"license" : "proprietary",
	"require" : {
		"php" : ">=5.4.16",
		"cakephp/cakephp" : "~3.0",
		"mobiledetect/mobiledetectlib" : "2.*",
		"cakephp/migrations" : "~1.0",
		"cakephp/plugin-installer" : "dev-master",
		"cake17/cakephp-recaptcha" : "dev-master",
		"phantomwatson/cakephp-autologin": "dev-master",
		"phantomwatson/ether-markov": "*",
		"phpunit/phpunit": "*",
		"cakephp/cakephp-codesniffer": "*",
		"oyejorge/less.php": "~1.5",
		"flesler/jquery.scrollto": "*",
		"twbs/bootstrap": "~3.0",
		"gourmet/common-mark": "~1.0",
		"xety/cake3-cookieauth": "1.*"
	},
	"require-dev" : {
		"psy/psysh" : "@stable",
		"cakephp/debug_kit" : "~3.0",
		"cakephp/bake" : "~1.0",
		"league/html-to-markdown" : "~4.0"
	},
	"suggest" : {
		"phpunit/phpunit" : "Allows automated tests to be run without system-wide install.",
		"cakephp/cakephp-codesniffer" : "Allows to check the code against the coding standards used in CakePHP."
	},
	"autoload" : {
		"psr-4" : {
			"App\\" : "src",
			"AutoLogin\\" : "./vendor/phantomwatson/cakephp-autologin/src"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"App\\Test\\" : "tests",
			"Cake\\Test\\" : "./vendor/cakephp/cakephp/tests",
			"HTML_To_Markdown\\": "./vendor/league/html-to-markdown"
		}
	},
	"scripts" : {
		"post-install-cmd" : "App\\Console\\Installer::postInstall",
		"post-autoload-dump": "Cake\\Composer\\Installer\\PluginInstaller::postAutoloadDump"
	},
	"minimum-stability" : "dev",
	"prefer-stable" : true
}
